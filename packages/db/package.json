{
  "name": "@todoai/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": {
        "types": "./dist/client.d.ts",
        "default": "./dist/client.js"
      },
      "require": {
        "types": "./dist/client.d.cts",
        "default": "./dist/client.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup && pnpm exec prisma generate",
    "dev": "tsup --watch",
    "generate": "pnpm exec prisma generate",
    "migrate:dev": "pnpm exec prisma migrate dev",
    "migrate:deploy": "pnpm exec prisma migrate deploy",
    "db:push": "pnpm exec prisma db push",
    "db:seed": "pnpm exec tsx prisma/seed.ts",
    "studio": "pnpm exec prisma studio",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0"
  },
  "devDependencies": {
    "@todoai/config": "workspace:*",
    "prisma": "^5.22.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}

